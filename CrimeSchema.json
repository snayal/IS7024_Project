{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "type": "array",
  "items": {
    "$ref": "#/definitions/CrimeElement"
  },
  "definitions": {
    "CrimeElement": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "instanceid": {
          "type": "string"
        },
        "incident_no": {
          "type": "string"
        },
        "date_reported": {
          "type": "string",
          "format": "date-time"
        },
        "date_from": {
          "type": "string",
          "format": "date-time"
        },
        "date_to": {
          "type": "string",
          "format": "date-time"
        },
        "clsd": {
          "$ref": "#/definitions/Clsd"
        },
        "ucr": {
          "type": "string",
          "format": "integer"
        },
        "dst": {
          "$ref": "#/definitions/DstUnion"
        },
        "beat": {
          "type": "string",
          "format": "integer"
        },
        "offense": {
          "type": "string"
        },
        "location": {
          "type": "string"
        },
        "floor": {
          "type": "string"
        },
        "side": {
          "type": "string"
        },
        "opening": {
          "type": "string"
        },
        "hate_bias": {
          "$ref": "#/definitions/HateBias"
        },
        "dayofweek": {
          "$ref": "#/definitions/Dayofweek"
        },
        "rpt_area": {
          "type": "string",
          "format": "integer"
        },
        "cpd_neighborhood": {
          "type": "string"
        },
        "neighborhood": {
          "$ref": "#/definitions/Neighborhood"
        },
        "weapons": {
          "type": "string"
        },
        "date_of_clearance": {
          "type": "string",
          "format": "date-time"
        },
        "hour_from": {
          "type": "string"
        },
        "hour_to": {
          "type": "string"
        },
        "address_x": {
          "type": "string"
        },
        "victim_age": {
          "$ref": "#/definitions/Age"
        },
        "victim_race": {
          "$ref": "#/definitions/Race"
        },
        "victim_ethnicity": {
          "$ref": "#/definitions/Ethnicity"
        },
        "victim_gender": {
          "$ref": "#/definitions/Gender"
        },
        "suspect_age": {
          "$ref": "#/definitions/Age"
        },
        "totalnumbervictims": {
          "type": "string",
          "format": "integer"
        },
        "ucr_group": {
          "$ref": "#/definitions/UcrGroup"
        },
        "community_council_neighborhood": {
          "$ref": "#/definitions/Neighborhood"
        },
        "zip": {
          "type": "string",
          "format": "integer"
        },
        "suspect_race": {
          "$ref": "#/definitions/Race"
        },
        "suspect_ethnicity": {
          "$ref": "#/definitions/Ethnicity"
        },
        "suspect_gender": {
          "$ref": "#/definitions/Gender"
        },
        "totalsuspects": {
          "type": "string",
          "format": "integer"
        },
        "theft_code": {
          "$ref": "#/definitions/TheftCode"
        }
      },
      "required": [
        "community_council_neighborhood",
        "date_from",
        "date_reported",
        "hour_from",
        "incident_no",
        "instanceid",
        "neighborhood",
        "offense",
        "suspect_age",
        "ucr",
        "ucr_group",
        "victim_age",
        "zip"
      ],
      "title": "CrimeElement"
    },
    "DstUnion": {
      "anyOf": [
        {
          "$ref": "#/definitions/DstEnum"
        },
        {
          "type": "string",
          "format": "integer"
        }
      ],
      "title": "DstUnion"
    },
    "Clsd": {
      "type": "string",
      "enum": [
        "Z--EARLY CLOSED",
        "J--CLOSED",
        "F--CLEARED BY ARREST - ADULT",
        "D--VICTIM REFUSED TO COOPERATE",
        "H--WARRANT ISSUED",
        "G--CLEARED BY ARREST - JUVENILE",
        "B--PROSECUTION DECLINED",
        "K--UNFOUNDED",
        "I--INVESTIGATION PENDING"
      ],
      "title": "Clsd"
    },
    "Neighborhood": {
      "type": "string",
      "enum": [
        "N/A",
        "ROSELAWN",
        "EAST PRICE HILL",
        "COLLEGE HILL",
        "NORTH AVONDALE - PADDOCK HILLS",
        "WEST END",
        "AVONDALE",
        "CUF",
        "WESTWOOD",
        "OVER-THE-RHINE",
        "WEST PRICE HILL",
        "MT. WASHINGTON",
        "OAKLEY",
        "HARTWELL",
        "BOND HILL",
        "PLEASANT RIDGE",
        "EAST WESTWOOD",
        "KENNEDY HEIGHTS",
        "CAMP WASHINGTON",
        "VILLAGES AT ROLL HILL",
        "CLIFTON",
        "WALNUT HILLS",
        "MILLVALE",
        "DOWNTOWN",
        "SPRING GROVE - WINTON HILLS",
        "PADDOCK HILLS",
        "OTR",
        "NORTHSIDE",
        "MOUNT WASHINGTON",
        "SOUTH FAIRMOUNT - WESTWOOD",
        "AVONDALE - NORTH AVONDALE",
        "EAST WALNUT HILLS"
      ],
      "title": "Neighborhood"
    },
    "Dayofweek": {
      "type": "string",
      "enum": [
        "WEDNESDAY",
        "THURSDAY",
        "SUNDAY",
        "FRIDAY",
        "MONDAY",
        "SATURDAY",
        "TUESDAY"
      ],
      "title": "Dayofweek"
    },
    "DstEnum": {
      "type": "string",
      "enum": [
        "CENTRAL BUSINESS"
      ],
      "title": "DstEnum"
    },
    "HateBias": {
      "type": "string",
      "enum": [
        "N--NO BIAS/NOT APPLICABLE",
        "88--DOMESTIC VIOLENCE"
      ],
      "title": "HateBias"
    },
    "Age": {
      "type": "string",
      "enum": [
        "OVER 70",
        "31-40",
        "41-50",
        "18-25",
        "26-30",
        "UNKNOWN",
        "UNDER 18",
        "ADULT (18+)",
        "51-60",
        "61-70",
        "JUVENILE (UNDER 18)"
      ],
      "title": "Age"
    },
    "Ethnicity": {
      "type": "string",
      "enum": [
        "NOT OF HISPANIC ORIG",
        "UNKNOWN",
        "HISPANIC ORIGIN",
        "WHITE AMERICAN",
        "AFRICAN AMERICAN",
        "WHITE NOT OF HISPANI"
      ],
      "title": "Ethnicity"
    },
    "Gender": {
      "type": "string",
      "enum": [
        "FEMALE",
        "MALE",
        "UNKNOWN"
      ],
      "title": "Gender"
    },
    "Race": {
      "type": "string",
      "enum": [
        "BLACK",
        "WHITE",
        "UNKNOWN",
        "HISPANIC",
        "ASIAN/PACIFIC ISLAND",
        "AMERICAN INDIAN/ALAS",
        "ASIAN OR PACIFIC ISL"
      ],
      "title": "Race"
    },
    "TheftCode": {
      "type": "string",
      "enum": [
        "24O-MOTOR VEHICLE THEFT",
        "23F-THEFT FROM MOTOR VEHICLE",
        "23D-THEFT FROM BUILDING",
        "23G-THEFT OF MOTOR VEHICLE PARTS OR ACCESSORIES",
        "23H-ALL OTHER LARCENY",
        "24I-THEFT OF LICENSE PLATE",
        "23C-SHOPLIFTING",
        "23B-PURSE-SNATCHING",
        "23A-POCKET-PICKING"
      ],
      "title": "TheftCode"
    },
    "UcrGroup": {
      "type": "string",
      "enum": [
        "BURGLARY/BREAKING ENTERING",
        "PART 2 MINOR",
        "THEFT",
        "ROBBERY",
        "AGGRAVATED ASSAULTS",
        "RAPE",
        "UNAUTHORIZED USE",
        "HOMICIDE"
      ],
      "title": "UcrGroup"
    }
  }
}
